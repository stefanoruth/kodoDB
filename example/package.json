{
	"name": "example",
	"version": "0.1.0",
	"license": "MIT",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"private": true,
	"dependencies": {
		"@types/node": "^10.12.18",
		"typescript": "^3.2.4"
	},
	"scripts": {
		"start": "node dist/index.js",
		"build": "rm -rf dist/* && tsc",
		"dev": "rm -rf dist/* && tsc-watch --onSuccess \"node dist/index.js\""
	},
	"prettier": {
		"semi": false,
		"useTabs": true,
		"singleQuote": true,
		"trailingComma": "es5",
		"proseWrap": "always",
		"jsxBracketSameLine": true,
		"bracketSpacing": true,
		"arrowParens": "avoid",
		"printWidth": 120
	},
	"devDependencies": {
		"tsc-watch": "^2.1.2"
	}
}
